{
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base",
        "plugin:react/recommended"
    ],
    "rules": {
        "indent": ["error", 4],
        "no-bitwise": 0,
        "no-alert": 0
    },
    "env": {
        "browser": true,
        "jest": true
    },
    "plugins": [
        "jest"
    ]
}
